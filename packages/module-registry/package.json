{
  "name": "@nova/module-registry",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "module-registry.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./defaults": {
      "types": "./dist/defaults.d.ts",
      "import": "./dist/defaults.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/defaults.ts --format esm --dts",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  }
}
